{
  "name": "uoh-ngx-theme-app",
  "version": "3.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build_lib": "ng build --prod @uoh/ngx-theme && node build-theme && node build-assets",
    "ver_patch_lib": "cd projects/uoh/ngx-theme/ && npm version patch",
    "ver_minor_lib": "cd projects/uoh/ngx-theme/ && npm version minor",
    "ver_major_lib": "cd projects/uoh/ngx-theme/ && npm version major",
    "ver_commit": "git add . && git commit -m 'New version'",
    "git_tags": "git push --tags && git push",
    "ver_patch": "npm version patch && npm run ver_patch_lib && npm run ver_commit && npm run git_tags",
    "ver_minor": "npm version minor && npm run ver_minor_lib && npm run ver_commit && npm run git_tags",
    "ver_major": "npm version major && npm run ver_major_lib && npm run ver_commit && npm run git_tags",
    "npm_pack": "cd dist/uoh/ngx-theme && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "publish_pack": "npm run package && npm publish dist/uoh/ngx-theme/uoh-ngx-theme-*.tgz --access public",
    "publish_patch": "npm run ver_patch && npm run publish_pack",
    "publish_minor": "npm run ver_minor && npm run publish_pack",
    "publish_major": "npm run ver_major && npm run publish_pack",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.2",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "^8.1.2",
    "@angular/compiler": "^8.1.2",
    "@angular/core": "^8.1.2",
    "@angular/forms": "^8.1.2",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "^8.1.2",
    "@angular/platform-browser-dynamic": "^8.1.2",
    "@angular/router": "^8.1.2",
    "ajv": "^6.10.2",
    "core-js": "^2.6.9",
    "rxjs": "^6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular-devkit/build-ng-packagr": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "^8.1.2",
    "@angular/language-service": "^8.1.2",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.6.8",
    "codelyzer": "^5.0.1",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.1.0",
    "protractor": "^5.4.2",
    "ts-node": "^8.3.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "typescript": "~3.4.5"
  }
}
