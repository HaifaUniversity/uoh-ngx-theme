{
  "name": "uoh-ngx-theme-app",
  "version": "1.1.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build_lib": "ng build --prod @uoh/ngx-theme && node build-theme && node build-assets",
    "ver_patch_lib": "cd projects/uoh/ngx-theme/ && npm version patch",
    "ver_minor_lib": "cd projects/uoh/ngx-theme/ && npm version minor",
    "ver_major_lib": "cd projects/uoh/ngx-theme/ && npm version major",
    "git_tags": "git push --tags && git push",
    "ver_patch": "npm version patch && npm run ver_patch_lib && npm run git_tags",
    "ver_minor": "npm version minor && npm run ver_minor_lib && npm run git_tags",
    "ver_major": "npm version major && npm run ver_major_lib && npm run git_tags",
    "npm_pack": "cd dist/uoh/ngx-theme && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "publish_pack": "npm run package && npm publish dist/uoh/ngx-theme/uoh-ngx-theme-*.tgz --access public",
    "publish_patch": "npm run ver_patch && npm run publish_pack",
    "publish_minor": "npm run ver_minor && npm run publish_pack",
    "publish_major": "npm run ver_major && npm run publish_pack",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.2",
    "@angular/cdk": "^7.2.2",
    "@angular/common": "^7.2.2",
    "@angular/compiler": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/material": "^7.2.2",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/router": "^7.2.2",
    "core-js": "^2.6.1",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.3",
    "@angular-devkit/build-ng-packagr": "^0.12.3",
    "@angular/cli": "~7.2.3",
    "@angular/compiler-cli": "^7.2.2",
    "@angular/language-service": "^7.2.2",
    "@types/jasmine": "^2.8.14",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~8.9.4",
    "codelyzer": "^4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.6.0",
    "protractor": "^5.4.2",
    "ts-node": "~5.0.1",
    "tsickle": "^0.34.0",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~3.2.4"
  }
}
