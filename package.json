{
  "name": "uoh-ngx-theme-app",
  "version": "11.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build_schematics": "cd projects/uoh/ngx-theme && npm run build",
    "build_lib": "ng build --prod @uoh/ngx-theme && node build-theme && node build-assets && npm run build_schematics",
    "ver_patch_lib": "cd projects/uoh/ngx-theme/ && npm version patch",
    "ver_minor_lib": "cd projects/uoh/ngx-theme/ && npm version minor",
    "ver_major_lib": "cd projects/uoh/ngx-theme/ && npm version major",
    "ver_commit": "git add . && git commit -m 'New version'",
    "git_tags": "git push --tags && git push",
    "ver_patch": "npm version patch && npm run ver_patch_lib && npm run ver_commit && npm run git_tags",
    "ver_minor": "npm version minor && npm run ver_minor_lib && npm run ver_commit && npm run git_tags",
    "ver_major": "npm version major && npm run ver_major_lib && npm run ver_commit && npm run git_tags",
    "npm_pack": "cd dist/uoh/ngx-theme && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "publish_pack": "npm run package && npm publish dist/uoh/ngx-theme/uoh-ngx-theme-*.tgz --access public",
    "publish_patch": "npm run ver_patch && npm run publish_pack",
    "publish_minor": "npm run ver_minor && npm run publish_pack",
    "publish_major": "npm run ver_major && npm run publish_pack",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.4",
    "@angular/cdk": "^11.0.2",
    "@angular/common": "^11.0.4",
    "@angular/compiler": "^11.0.4",
    "@angular/core": "^11.0.4",
    "@angular/forms": "^11.0.4",
    "@angular/material": "^11.0.2",
    "@angular/platform-browser": "^11.0.4",
    "@angular/platform-browser-dynamic": "^11.0.4",
    "@angular/router": "^11.0.4",
    "ajv": "^6.12.6",
    "core-js": "^2.6.12",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular-devkit/core": "^11.0.4",
    "@angular/cli": "^11.0.4",
    "@angular/compiler-cli": "^11.0.4",
    "@angular/language-service": "^11.0.4",
    "@schematics/angular": "^11.0.3",
    "@types/jasmine": "^3.6.2",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.19.8",
    "codelyzer": "^6.0.1",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.0.3",
    "protractor": "^7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "~4.0.5"
  }
}
